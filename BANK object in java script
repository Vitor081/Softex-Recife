function Banco(conta, saldo, tipo_de_conta, agencia, numero_da_conta) {
    this.conta = conta;
    this.saldo = saldo;
    this.tipo_de_conta = tipo_de_conta;
    this.agencia = agencia;
    this.numero_da_conta = numero_da_conta;

     

    saldoAtual = function(saldoAtual){
        saldoAtual = this.saldo
    }

    deposito = function (deposito) {
        saldo_final = this.saldoAtual + deposito
        console.log('O seu saldo é: ${saldo_final}')
    }

    saque = function (saque) {
        saque = saldo_final - saque
        console.log('O seu saldo pós saque é ${saque}')
    }

    conta = function (conta) {
        console.log(numero_da_conta)
    }

}

let nova = new Banco(14532, 1500, corrente, 1458, 28796 )
console.saldoAtual()
console.deposito()
console.saque()
ConstantSourceNode.conta()
